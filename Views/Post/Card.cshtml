@model CardViewModel
@{
    Layout = "__HomeLayout";
    ViewData["Title"] = "Details";

}

<hr class="border border-top" />
<div class="card-body">

    <form method="get">
        <div class="form-group">
            <label asp-for="Surname" class="control-label"></label>
            <input asp-for="Surname" readonly class="form-control" />

        </div>
        <div class="form-group">
            <label asp-for="iniatials" class="control-label"></label>
            <input asp-for="iniatials" readonly class="form-control" />

        </div>
 
    </form>
</div>
